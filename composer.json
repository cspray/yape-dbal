{
  "name": "cspray/yape-dbal",
  "description": "An extension of cspray/yape that allows storing generated Enums in your Doctrine Entities",
  "keywords": ["enum", "doctrine", "dbal"],
  "homepage": "https://cspray.io/yape",
  "license": "MIT",
  "authors": [
    {
      "name": "Charles Sprayberry",
      "email": "yape@cspray.io",
      "homepage": "http://cspray.io",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "cspray/yape": "^3.0",
    "doctrine/dbal": "^2.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Cspray\\Yape\\Dbal\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\Yape\\Dbal\\": "test/"
    }
  }
}